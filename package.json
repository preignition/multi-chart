  "snowpack": {
    "webDependencies": [
      "@morbidick/lit-element-notify",
      "@polymer/polymer/lib/utils/mixin.js",
      "@polymer/polymer/lib/utils/case-map.js",
      "@polymer/polymer/lib/utils/async.js",
      "@polymer/polymer/lib/utils/debounce.js",
      "@preignition/preignition-mixin",
      "@preignition/preignition-demo",
      "@preignition/preignition-util",
      "@vaadin/router",
      "@vaadin/vaadin-tabs",
      "pwa-helpers/media-query.js",
      "tslib",
      "marked",
      "d3-array",
      "d3-axis",
      "d3-brush",
      "d3-fetch",
      "d3-interpolate",
      "d3-selection",
      "d3-selection-multi",
      "d3-shape",
      "d3-transition",
      "d3-format",
      "d3-time-format",
      "d3-random",
      "d3-scale",
      "d3-time",
      "d3-scale-chromatic",
      "d3-svg-legend",
      "d3-zoom",
      "lit-element",
      "lit-html/directives/cache.js",
      "lit-html/directives/unsafe-html.js",
      "lit-html/directives/until.js",
      "lit-html/directives/style-map.js",
      "lit-html"
    ],
    "dedupe": [
      "lit-html",
      "lit-element",
      "marked",
      "d3-selection",
      "d3-transition"
    ]
  },
  "_snowpack": {
    "exclude": [
      "**/node_modules/**/*",
      "**/bower_components/**/*",
      "**/coverage/**/*",
      "**/images/**/*",
      "**/docs/**/*.@(js)",
      "**/test/*",
      "**/*.@(spec|test).@(js|mjs)"
    ],
    "install": [
      "@morbidick/lit-element-notify",
      "@polymer/polymer/lib/utils/mixin.js",
      "@polymer/polymer/lib/utils/case-map.js",
      "@polymer/polymer/lib/utils/async.js",
      "@polymer/polymer/lib/utils/debounce.js",
      "@preignition/preignition-mixin",
      "@preignition/preignition-demo",
      "@preignition/preignition-util",
      "@vaadin/router",
      "@vaadin/vaadin-tabs",
      "pwa-helpers/media-query.js",
      "tslib",
      "marked",
      "d3-array",
      "d3-axis",
      "d3-brush",
      "d3-fetch",
      "d3-interpolate",
      "d3-selection",
      "d3-selection-multi",
      "d3-shape",
      "d3-transition",
      "d3-format",
      "d3-time-format",
      "d3-random",
      "d3-scale",
      "d3-time",
      "d3-scale-chromatic",
      "d3-svg-legend",
      "d3-zoom",
      "lit-element",
      "lit-html/directives/cache.js",
      "lit-html/directives/unsafe-html.js",
      "lit-html/directives/until.js",
      "lit-html/directives/style-map.js",
      "lit-html"
    ],
    "installConfigs": {
      "rollup": {
        "dedupe": [
          "lit-html",
          "lit-element",
          "marked",
          "d3-selection",
          "d3-transition"
        ]
      }
    }
  },
  "scripts": {
    "start": "es-dev-server -v",
    "generate-docs-json": "wca analyze src/**/*.js  --outFiles docs/{tagname}.json ",
    "generate-docs-md": "wca analyze src/**/*.js  --outFiles docs/{tagname}.md ",
    "generate-docs:helper": "wca analyze src/helper/**/*.js --outFile docs/helper.json",
    "generate-docs:wrapper": "wca analyze src/d3-wrapper/**/*.js --outFile docs/wrapper.json",
    "generate-docs:chart": "wca analyze src/chart/**/*.js --outFile docs/chart.json",
    "generate-docs:container": "wca analyze src/container/**/*.js --outFile docs/container.json",
    "generate-docs": "npm run generate-docs-md && npm run generate-docs-json && npm run generate-docs:helper && npm run generate-docs:wrapper && npm run generate-docs:container && npm run generate-docs:chart",
    "prepublishOnly": "npm run generate-docs && pnpx snowpack i",
    "postpublish": "git push && git push --tags",
    "snowpack": "pnpx snowpack",
    "test": "karma start --coverage",
    "test:compatibility": "karma start --compatibility all --coverage"
  },
  "author": "christophe.geiser <christophe@preignition.org>"
}
